<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xxhoz.dao.HosUsersDao">
  <select id="queryUser" resultType="HosUser">
    select * from hos_users where user_name=#{userName} and user_password =#{userPassword}
  </select>
</mapper>
